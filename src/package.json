{
  "devDependencies": {
    "electron": "^28.2.2",
    "electron-builder": "^24.9.1",
    "eslint": "^8.35.0"
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "chroma-js": "^2.4.2",
    "echarts": "^5.4.3",
    "leaflet": "^1.9.4",
    "ws": "^8.16.0"
  },
  "name": "rts-map",
  "version": "0.1.0-alpha.4",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "rts-map",
    "appId": "tw.exptech.rts-map",
    "asar": true,
    "files": [
      "resources/**/*",
      "scripts/*",
      "stylesheets/*",
      "views/*",
      "app.ico",
      "main.js",
      "package-lock.json",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "publish": null,
      "target": "portable",
      "icon": "app.ico"
    },
    "linux": {
      "publish": null,
      "target": "deb",
      "category": "Development",
      "maintainer": "ExpTechTW"
    },
    "directories": {
      "output": "../dist"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ExpTechTW/TREM/rts-map"
  },
  "author": "ExpTechTW",
  "license": "AGPL-3.0",
  "description": "rts map viewer"
}
