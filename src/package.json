{
  "devDependencies": {
    "electron": "^22.0.3",
    "electron-builder": "^23.6.0",
    "eslint": "^8.32.0"
  },
  "dependencies": {
    "chroma-js": "^2.4.2",
    "leaflet": "^1.9.3"
  },
  "name": "rts-map",
  "version": "0.0.4",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "rts-map",
    "appId": "tw.exptech.rts-map",
    "asar": false,
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "publish": null,
      "target": ["nsis", "portable"],
      "icon": "app.ico"
    },
    "linux": {
      "publish": null,
      "target": "deb",
      "category": "Development",
      "maintainer": "ExpTechTW"
    },
    "directories": {
      "output": "dist"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ExpTechTW/TREM/rts-map"
  },
  "author": "ExpTechTW",
  "license": "AGPL-3.0",
  "description": "rts map viewer"
}
